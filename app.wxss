/**app.wxss**/
.container {
  height: 100%;
  box-sizing: border-box;
} 

.search{
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 80rpx;
    padding: 10rpx 50rpx;
    font-size: 14px;
    background: #26a2ff;
    box-sizing: border-box;
    z-index: 999;
}
.search input{
    width: 100%;
    height: 100%;
    border-radius: 6rpx;
    background: #fff;
    padding-left: 10rpx;
    box-sizing: border-box;
}

